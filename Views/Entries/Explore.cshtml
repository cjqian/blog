@inject UserManager<ApplicationUser> UserManager
@model IEnumerable<blog.Models.Entry>

@{
    ViewData["Title"] = "Explore";
}

        @foreach (var item in Model.Where(n => n.IsPublic == true).Reverse())
        {
            @await Component.InvokeAsync("DetailEntry", new { entry = item, showComments = false })

        }
